.s-item-container .s-item {
  white-space: nowrap;
  height: 220rpx;
}
.s-item-container .s-item .s-btn-l {
  width: 90rpx;
  height: 100%;
  box-sizing: border-box;
  padding: 66rpx 0;
  display: inline-block;
  vertical-align: top;
}
.s-item-container .s-item .s-btn-l .s-btn-c {
  height: 100%;
  width: 90rpx;
  padding: 26rpx 27rpx;
  box-sizing: border-box;
}
.s-item-container .s-item .s-icon {
  display: inline-block;
  width: 192rpx;
  min-height: 192rpx;
  box-sizing: border-box;
  padding: 14rpx 0;
  vertical-align: top;
  margin-right: 24rpx;
}
.s-item-container .s-item .s-icon .s-icon-img {
  width: 100%;
  height: 192rpx;
  display: block;
  outline-width: 0px;
  vertical-align: top;
}
.s-item-container .s-item .s-item-detail {
  display: inline-block;
  width: 440px;
  box-sizing: border-box;
  padding-top: 20rpx;
  padding-right: 30rpx;
  vertical-align: top;
}
.s-item-container .s-item .s-item-detail .s-i-t {
  color: #282828;
  line-height: 44rpx;
  font-size: 30rpx;
  white-space: pre-line;
}
.s-item-container .s-item .s-item-detail .s-i-startDate {
  margin-top: 7rpx;
  color: #999999;
  font-size: 26rpx;
}
.s-item-container .s-item .s-item-detail .s-i-p-n {
  
  justify-content: space-between;
  margin-top: 15rpx;
  display: flex;
}
.s-item-container .s-item .s-item-detail .s-i-p-n .s-i-p {
  color: #fc3b22;
  font-size: 30rpx;
  display:inline-block;
    margin-right:330rpx;
    
}
.s-item-container .s-item .s-item-detail .s-i-p-n .s-i-n {
  color: #999999;
  font-size: 26rpx;
  position:absolute;
  right:30rpx;
  
}
.s-item-container .s-item .s-item-delete {
  display: inline-block;
  vertical-align: top;
  width: 130rpx;
  height: 100%;
  background-color: #fc3b22;
  color: #fff;
  text-align: center;
  line-height: 220rpx;
}
.s-item-container .s-item .s-item-detail-d {
  display: inline-block;
  vertical-align: top;
  width: 320rpx;
  box-sizing: border-box;
  padding-top: 24rpx;
}
.s-item-container .s-item .s-item-detail-d .s-i-compute .s-i-c-cut,
.s-item-container .s-item .s-item-detail-d .s-i-compute .s-i-c-cut-d,
.s-item-container .s-item .s-item-detail-d .s-i-compute .s-i-c-num,
.s-item-container .s-item .s-item-detail-d .s-i-compute .s-i-c-add {
  width: 52rpx;
  height: 52rpx;
  display: inline-block;
  text-align: center;
  font-size: 26rpx;
  padding-top: 8rpx;
  box-sizing: border-box;
}
.s-item-container .s-item .s-item-detail-d .s-i-compute .s-i-c-cut {
  border-top: 1rpx solid #ababab;
  border-left: 1rpx solid #ababab;
  border-bottom: 1rpx solid #ababab;
  color: #999999;
}

.s-item-container .s-item .s-item-detail-d .s-i-compute .s-i-c-cut-d {
  border-top: 1rpx solid #686868;
  border-left: 1rpx solid #686868;
  border-bottom: 1rpx solid #686868;
  color: #999999;
}

.s-item-container .s-item .s-item-detail-d .s-i-compute .s-i-c-num {
  border-top: 1rpx solid #686868;
  border-left: 1rpx solid #686868;
  border-bottom: 1rpx solid #686868;
  color: #282828;
  width:72rpx;
}
.s-item-container .s-item .s-item-detail-d .s-i-compute .s-i-c-add {
  border: 1rpx solid #686868;
  color: #282828;
}
.s-item-container .s-item .s-item-detail-d .s-i-startDate {
  margin-top: 7rpx;
  color: #999999;
  font-size: 26rpx;
  margin-top: 20rpx;
}
.s-item-container .s-item .s-item-detail-d .s-i-p-n {
  margin-top: 30rpx;
  color: #fc3b22;
  font-size: 30rpx;
}
.shoppingCart {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  min-height: 98rpx;
  background-color: #fff;
  line-height: 98rpx;
  color: #fff;
  font-size: 30rpx;
  z-index:10;
}
.shoppingCart .s-c-choose {
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  
}
.shoppingCart .s-c-choose .s-btn-cart {
  width: 90rpx;
  height: 100%;
  box-sizing: border-box;
  padding-top: 31rpx;
  padding-left: 27rpx;
}
.shoppingCart .s-c-choose .s-btn-cart .s-btn-c {
  margin-right: 30rpx;
}
.shoppingCart .s-c-choose .s-btn-cart .s-btn-c .s-btn-nochecked {
  width: 36rpx;
  height: 36rpx;
  border: 2rpx solid #686868;
  border-radius: 50%;
  box-sizing: border-box;
}
.shoppingCart .s-c-choose .s-btn-cart .s-btn-c .s-btn-checked {
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  box-sizing: border-box;
  color: #fff;
}
 .s-btn-checked .icon{
  width: 36rpx;
  height: 36rpx;
}

.shoppingCart .s-c-choose .s-c-text {
  color: #686868;
}
.shoppingCart .s-c-price {
  position: absolute;
  right: 290rpx;
  top: 0;
  height: 100%;
  color: #fc3b22;
}
.shoppingCart .s-c-pay {
  position: absolute;
  right: 0;
  top: 0;
  width: 240rpx;
  height: 100%;
  background-color: #fc3b22;
  text-align: center;
}